{
    "manifest_version": 3,
    "name": "Link Opener",
    "version": "1.0",
    "description": "Opens links matching a specific CSS selector from a given URL.",
    "permissions": [
      "tabs",
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "48": "icon.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "icon.png"
      }
    }
  }
  